{
  "properties": {
    "securityRules": [
      {
        "name": "AllowAllInbound",
        "properties": {
          "access": "Allow",
          "destinationAddressPrefix": "*",
          "destinationPortRange": "*",
          "direction": "Inbound",
          "priority": 1000,
          "protocol": "*",
          "sourceAddressPrefix": "0.0.0.0/0",
          "sourcePortRange": "*"
        }
      },
      {
        "name": "AllowAllOutbound",
        "properties": {
          "access": "Allow",
          "destinationAddressPrefix": "*",
          "destinationPortRange": "*",
          "direction": "Outbound",
          "priority": 1001,
          "protocol": "*",
          "sourceAddressPrefix": "*",
          "sourcePortRange": "*"
        }
      }
    ]
  }
}

